    "n = int(input())\n",
    "\n",
    "\n",
    "def facto(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return n*facto(n-1)\n",
    "    \n",
    "print(facto(n))\n"
   ]
