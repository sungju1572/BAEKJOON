   "source": [
    "N = int(input())\n",
    "a = 1\n",
    "sum = 0\n",
    "while True :\n",
    "    sum += a\n",
    "    if N <= sum :\n",
    "        b = sum - N\n",
    "        if a % 2 ==0:\n",
    "            print(str(a-b) + \"/\" + str(b+1))\n",
    "            break\n",
    "        else :\n",
    "            print(str(b+1) + \"/\" + str(a-b))\n",
    "            break\n",
    "    else:\n",
    "        a+=1"
   ]
  }
