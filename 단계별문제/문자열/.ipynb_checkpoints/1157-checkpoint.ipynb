    "N = input()\n",
    "N = N.upper()\n",
    "\n",
    "n = \"\".join(dict.fromkeys(N))\n",
    "\n",
    "a=[]\n",
    "b=[]\n",
    "for i in n:\n",
    "    a.append(N.count(i))\n",
    "    \n",
    "for i in a:\n",
    "    if i == max(a):\n",
    "        b.append(i)\n",
    "    \n",
    "if len(b) != len(set(b)):\n",
    "    print(\"?\")\n",
    "else:\n",
    "    c = a.index(max(a))\n",
    "    print(n[c])\n",
    "    \n",
    "    \n"
   ]
