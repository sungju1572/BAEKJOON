    "\n",
    "\n",
    "lower = string.ascii_lowercase\n",
    "\n",
    "count = 0\n",
    "for i in b:\n",
    "    if a.count(i)>0:\n",
    "        count+= a.count(i)\n",
    "        a = a.replace(i,\" \")\n",
    "          \n",
    "for i in lower:\n",
    "    count+= a.count(i)\n",
    "    \n",
    "print(count)\n"
